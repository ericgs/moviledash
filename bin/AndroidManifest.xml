<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="i3.moviledash"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="i3.moviledash.Tablero"
            android:label="@string/title_activity_tablero" >
        </activity>
        <activity
            android:name="i3.moviledash.ABMHistoria"
            android:label="@string/title_activity_abmhistoria" >
            <intent-filter>
<!--                 <action android:name="android.intent.action.MAIN" /> -->

<!--                 <category android:name="android.intent.category.LAUNCHER" /> -->
            </intent-filter>
        </activity>
        <activity
            android:name="i3.moviledash.Dash"
            android:label="@string/title_activity_dash" >
        </activity>
        <activity
            android:name="i3.moviledash.Proyecto"
            android:label="@string/title_activity_proyecto" >
        </activity>
        <activity
            android:name="i3.moviledash.HistoriaListActivity"
            android:label="@string/title_historia_list" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="i3.moviledash.HistoriaDetailActivity"
            android:label="@string/title_historia_detail"
            android:parentActivityName=".HistoriaListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HistoriaListActivity" />
        </activity>
                             
    </application>

</manifest>